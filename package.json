{
	"name": "dotxx",
	"version": "0.9.8",
	"type": "module",
	"bin": {
		"dotx": "dist/cli.js"
	},
	"keywords": [
		"cli"
	],
	"repository": {
		"url": "https://github.com/Silent-Watcher/dotx"
	},
	"author": {
		"email": "backendwithali@gmail.com",
		"name": "Ali nazari"
	},
	"license": "MIT",
	"description": "",
	"scripts": {
		"tsc": "tsc",
		"dev": "tsx --watch src/cli.ts",
		"prebuild": "npm run tsc && npm run clean",
		"build": "tsup",
		"prestart": "npm run build",
		"start": "node dist/cli.js",
		"test": "vitest run --coverage",
		"test:watch": "vitest dev",
		"prelint": "biome format --write ./src/*.ts ./tests/*.ts",
		"lint": "biome lint ./src/*.ts ./tests/*.ts",
		"check": "biome check --write ./src/*.ts ./tests/*.ts",
		"prepare": "husky",
		"commit": "cz",
		"clean": "del-cli dist/ coverage/ *.log",
		"prepublishOnly": "npm run clean && npm run build"
	},
	"config": {
		"commitizen": {
			"path": "./node_modules/cz-conventional-changelog"
		}
	},
	"devDependencies": {
		"@biomejs/biome": "^2.1.4",
		"@dotenvx/dotenvx": "^1.48.4",
		"@types/command-exists": "^1.2.3",
		"cz-conventional-changelog": "^3.3.0",
		"del-cli": "^6.0.0",
		"husky": "^9.1.7",
		"lint-staged": "^16.1.5",
		"tsup": "^8.5.0",
		"tsx": "^4.20.3",
		"typescript": "^5.9.2",
		"vitest": "^3.2.4"
	},
	"dependencies": {
		"command-exists": "^1.2.9",
		"commander": "^14.0.0"
	}
}
