{
	"name": "spotenv",
	"version": "1.3.2",
	"type": "module",
	"bin": {
		"spotenv": "dist/cli.js"
	},
	"keywords": [
		"cli",
		"env-parser",
		"env-loader",
		"environment-variables",
		"env",
		"config-management",
		"config-loader",
		"ast-parsing",
		"ast"
	],
	"repository": {
		"url": "https://github.com/Silent-Watcher/spotenv"
	},
	"author": {
		"email": "backendwithali@gmail.com",
		"name": "Ali nazari"
	},
	"license": "MIT",
	"description": "Scan project for environment variable usage and generate .env.example",
	"scripts": {
		"tsc": "tsc",
		"dev": "node --watch src/cli.ts",
		"prebuild": "npm run tsc && npm run clean",
		"build": "tsup",
		"prestart": "npm run build",
		"start": "node dist/cli.js",
		"test": "vitest run --coverage",
		"test:watch": "vitest dev",
		"prelint": "biome format --write ./src/*.ts ./tests/*.ts",
		"lint": "biome lint ./src/*.ts ./tests/*.ts",
		"check": "biome check --write ./src/*.ts ./tests/*.ts",
		"prepare": "husky",
		"commit": "cz",
		"clean": "del-cli dist/ coverage/ *.log",
		"prepublishOnly": "npm run clean && npm run build"
	},
	"config": {
		"commitizen": {
			"path": "./node_modules/cz-conventional-changelog"
		}
	},
	"devDependencies": {
		"@biomejs/biome": "^2.1.4",
		"@dotenvx/dotenvx": "^1.48.4",
		"@types/babel__traverse": "^7.28.0",
		"@types/command-exists": "^1.2.3",
		"cz-conventional-changelog": "^3.3.0",
		"del-cli": "^6.0.0",
		"husky": "^9.1.7",
		"lint-staged": "^16.1.5",
		"tsup": "^8.5.0",
		"typescript": "^5.9.2",
		"vitest": "^3.2.4"
	},
	"dependencies": {
		"@babel/parser": "^7.28.0",
		"@babel/traverse": "^7.28.0",
		"chokidar": "^4.0.3",
		"commander": "^14.0.0",
		"fast-glob": "^3.3.3",
		"ora": "^8.2.0"
	}
}
